{
  "title": "Docker -- 从入门到实践",
  "author": "yeasy",
  "language": "zh-hans",
  "links": {
    "sidebar": {
      "https://tkstorm.com": "https://tkstorm.com"
    }
  },
  "styles": {
    "website": "styles/website.css"
  },
  "plugins": [
    "-livereload",
    "-highlight",
    "-sharing",
    "-search",
    "-lunr",
    "page-treeview",
    "image-captions",
    "github",
    "theme-comscore",
    "splitter",
    "prism",
    "katex",
    "search-plus",
    "emphasize",
    "multipart"
  ],
  "pluginsConfig": {
    "prism": {
      "lang": {
        "flow": "typescript"
      },
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ],
      "ignore": [
        "mermaid",
        "eval-js"
      ]
    },
    "image-captions": {
      "attributes": {
        "width": "600"
      },
      "caption": "图 _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
    },
    "github": {
      "url": "https://github.com/tkstorm/docker_practice"
    }
  }
}
